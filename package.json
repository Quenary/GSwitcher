{
  "name": "gswitcher",
  "version": "1.0.0",
  "main": "./dist/gswitcher/electron-app.js",
  "author": {
    "email": "evgeny.savin.ad@gmail.com",
    "name": "Evgeniy Savin",
    "url": "https://github.com/Quenary"
  },
  "description": "App provides controls over PC displays color settings depending on the active window.",
  "scripts": {
    "serve": "ng serve --open",
    "serve:electron": "npm run build && electron .",
    "build": "ng build --base-href ./ && tsc -b tsconfig.electron.json",
    "forge:make": "npm run build && electron-forge make && start out",
    "forge:package": "npm run build && electron-forge package && start out",
    "postinstall": "npx electron-rebuild"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "assert": false,
    "util": false
  },
  "private": true,
  "dependencies": {
    "active-win": "^7.7.2",
    "easy-auto-launch": "^6.0.2",
    "ffi-napi": "^4.0.3",
